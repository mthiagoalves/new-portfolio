generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Project {
  id               String @id @default(uuid())
  title            String @unique
  smallDescription String
  description      String
  order            Int

  createdAt DateTime @default(now())
  updateAt  DateTime @updatedAt
}
